{
  "name": "e2e",
  "version": "1.0.0",
  "description": "A repeatable set of E2E tests for automating QA checks on the bundler",
  "repository": "git@github.com:stackup-wallet/stackup-bundler.git",
  "private": true,
  "scripts": {
    "lint": "eslint . && tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check --ignore-path ./.eslintignore '**'",
    "prettier:fix": "prettier --write --ignore-path ./.eslintignore '**'",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "ethers": "^6.7.1",
    "userop": "^0.3.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^18.11.9",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.6.1",
    "prettier": "^2.8.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
